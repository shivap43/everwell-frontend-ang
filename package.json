		"eslint-plugin-import": "latest",
		"eslint-plugin-sonarjs": "^0.11.0",
		"husky": "^4.3.8",
		"jest": "27.2.3",
		"jest-preset-angular": "10.0.1",
		"json-server": "^0.17.0",
