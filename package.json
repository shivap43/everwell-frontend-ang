		"lint-staged": "^8.2.1",
		"ngrx-uml": "^1.0.2",
		"prettier": "^2.4.1",
		"prettier-stylelint": "^0.4.2",
		"sonarjs": "1.0.0",
		"stylelint": "^11.0.0",
		"ts-jest": "27.0.5",
