		"@ngrx/router-store": "^14.3.3",
		"@ngrx/store": "^14.3.3",
		"@ngrx/store-devtools": "^14.3.3",
		"@ngxs-labs/dispatch-decorator": "^4.0.2",
		"@ngxs/router-plugin": "^3.7.3",
		"@ngxs/storage-plugin": "^3.7.3",
		"@ngxs/store": "^3.7.3",
		"@nrwl/angular": "14.2.0",
		"@stomp/ng2-stompjs": "^8.0.0",
		"@stomp/rx-stomp": "^2.0.0",
		"@stomp/stompjs": "^7.0.0",
