		"@nrwl/jest": "14.2.0",
		"@nrwl/linter": "14.2.0",
		"@nrwl/node": "14.2.0",
		"@nrwl/tao": "14.2.0",
		"@nrwl/workspace": "14.2.0",
		"@types/express": "4.16.0",
		"@types/jest": "27.0.2",
		"@types/jquery": "3.3.29",
		"@types/json-server": "^0.14.0",
		"@types/node": "14.14.33",
		"@typescript-eslint/eslint-plugin": "~4.33.0",
		"@typescript-eslint/parser": "~4.33.0",
		"brotli-webpack-plugin": "^1.1.0",
		"dotenv": "10.0.0",
